#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int arr[n],i,x=0,b=0,c=0;
    for(i=0;i<n;i++)
    scanf("%d",&arr[i]);
    for(i=0;i<n;i++)
    if(arr[i]>=38)
    {
    {x=0;
    b=0;
        
        x=arr[i]%5;
        c=(arr[i]-x)+5;
        b=c-arr[i];
        if(b<3)
        arr[i]=arr[i]+b;
        
    }
}
    for(i=0;i<n;i++)
    printf("%d\n",arr[i]);
    return 0;
}
